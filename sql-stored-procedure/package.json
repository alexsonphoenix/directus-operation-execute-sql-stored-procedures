{
	"name": "directus-extension-sql-stored-procedure",
	"description": "This operation allows you to execute a stored procedure in your SQL database",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-operation"
	],
	"directus:extension": {
		"type": "operation",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"source": {
			"app": "src/app.js",
			"api": "src/api.js"
		},
		"host": "^10.1.3"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.3",
		"vue": "^3.3.4"
	}
}
